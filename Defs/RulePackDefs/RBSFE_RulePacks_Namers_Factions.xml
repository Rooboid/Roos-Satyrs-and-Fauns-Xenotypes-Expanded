	<!--====================== Fae Deity Type ======================-->
	
	<RulePackDef>
		<defName>NamerFae_DeityType</defName>
		<rulePack>
			<rulesStrings>
				<li>r_deityType->[FaeGod]</li>
				
				<li>FaeGod(p=8)->World Tree</li>
				<li>FaeGod->Mother Earth</li>
				<li>FaeGod->God</li>
				<li>FaeGod->Titan</li>
				<li>FaeGod->Primordial Deity</li>

			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
	<!--====================== Fae Ideology ======================-->
		
	<RulePackDef>
		<defName>NamerFae_Ideo</defName>
		<include>
			<li>NamerSatyrFaunBaseExpanded</li>
			<li MayRequire="Ludeon.RimWorld.Ideology">NamerIdeoGlobal</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_ideoName->[FaunPre][FaeIdeoSuf]</li>
				<li>r_ideoName->[SatyrPre][FaeIdeoSuf]</li>
			</rulesStrings>
			
			<rulesFiles>
				<li>FaeIdeoSuf->WordParts/Ideology/FaeIdeologySuffix</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>

</Defs>