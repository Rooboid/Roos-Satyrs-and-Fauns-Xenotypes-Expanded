<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--====================== Satyr / Faun Name Assets ======================-->
	<!-- All Files -->

	<RulePackDef>
		<defName>NamerSatyrFaunBaseExpanded</defName>
		<rulePack>
			<rulesFiles>
				<!-- General -->
				<li>FaunPre->Names/Faun/Prefix_Faun</li>
				<li>SatyrPre->Names/Satyr/Prefix_Satyr</li>
				<li>FaeSuf->WordParts/Factions/Suffix_FaeFaction</li>
				<li>FaeFac->Names/Factions/Unique_FaeFaction</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
  
	<!--====================== Fae Faction - influenced by Celtic & Greek Folklore ======================-->
	<!-- Fae Faction -->

	<RulePackDef>
		<defName>NamerFactionFaeTribe</defName>
		<include>
			<li>NamerSatyrFaunBaseExpanded</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=8)->[FaeFac]</li>
				<li>r_name->Travellers of [FaeFac]</li>
				<li>r_name->Nomads of [FaeFac]</li>
				<li>r_name->Fae of [FaeFac]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
	<!-- Fae Faction - Settlements -->
  
	<RulePackDef>
		<defName>NamerSettlementFaeTribe</defName>
		<include>
			<li>NamerSatyrFaunBaseExpanded</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name->[FaunPre][FaeSuf]</li>
				<li>r_name->[SatyrPre][FaeSuf]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
	<!--====================== Fae Leader ======================-->
	
	<RulePackDef>
		<defName>NamerFae_Leader</defName>
		<rulePack>
			<rulesStrings>
				<li>r_leaderTitle->[MinoLeader]</li>
				
				<li>MinoLeader->Paragon</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
