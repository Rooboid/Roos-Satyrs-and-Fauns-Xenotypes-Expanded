<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--====================== PawnKind Bases ======================-->
	<!-- Fae Base - Standard base for all fae units -->
	
	<PawnKindDef Name="FaeBase" Abstract="True">
		<!-- Basics -->
		<race>Human</race>
		<defaultFactionType>RBSFE_TribeFae</defaultFactionType>
		<maxGenerationAge>90</maxGenerationAge>
		<initialWillRange>0~1</initialWillRange>
		<initialResistanceRange>3~8</initialResistanceRange>
		<backstoryFiltersOverride>
			<li>
				<categoriesChildhood>
					<li>SatyrandFaun</li>
					<li>Tribal</li>
				</categoriesChildhood>
				<categoriesAdulthood>
					<li>SatyrandFaun</li>
					<li>Tribal</li>
				</categoriesAdulthood>
			</li>
		</backstoryFiltersOverride>
		<acceptArrestChanceFactor>1.2</acceptArrestChanceFactor>
		<chemicalAddictionChance>0.05</chemicalAddictionChance>
		<!-- Inventory -->
		<invNutrition>2.55</invNutrition>
		<invFoodDef>MealFine_Veg</invFoodDef>
		<!-- Hediffs -->
		<techHediffsMoney>0~3000</techHediffsMoney>
		<techHediffsTags>
			<li>Poor</li>
		</techHediffsTags>
		<techHediffsChance>0.02</techHediffsChance>
		<!-- Xenotype -->
		<xenotypeSet>
			<xenotypeChances Inherit="False">
				<RBSF_Satyr MayRequire="Ludeon.RimWorld.Biotech">1</RBSF_Satyr>
				<RBSF_Faun MayRequire="Ludeon.RimWorld.Biotech">1</RBSF_Faun>
				<LTS_Elfin MayRequire="LimeTreeSnake.Xenotech">0.05</LTS_Elfin>
			</xenotypeChances>
		</xenotypeSet>
		<!-- Apparel -->
		<apparelTags>
			<li>Fae</li>
		</apparelTags>
		<apparelDisallowTags>
		<li>IndustrialBasic</li>
		</apparelDisallowTags>
		<apparelAllowHeadgearChance>0.4</apparelAllowHeadgearChance>
		<gearHealthRange>0.5~1</gearHealthRange>
		<specificApparelRequirements>
			<li>
				<colorGenerator Class="ColorGenerator_Options">
					<options>
						<li>
							<weight>10</weight>
							<only>(57,116,67)</only><!-- Rich Green -->
						</li>
						<li>
							<weight>5</weight>
							<only>(60,70,50)</only><!-- Dark Green -->
						</li>
						<li>
							<weight>1</weight>
							<only>(150,70,60)</only><!-- Satyr Brown -->
						</li>
						<li>
							<weight>1</weight>
							<only>(201,92,54)</only><!-- Faun Brown -->
						</li>
					</options>
				</colorGenerator>
			</li>
		</specificApparelRequirements>
	</PawnKindDef>
	
	<!--====================== Fae Tribe - Peaceful Pawns ======================-->
	<!-- Unqiue, none-fighter pawns belonging to the Fae Tribe -->
	
	<!-- Fae - Peaceful pawns of satyr or faun xenotypes -->
	
	<PawnKindDef ParentName="FaeBase">
		<defName>RBSF_Fae</defName>
		<label>fae</label>
		<isFighter>False</isFighter>
		<favoriteColor>(57,116,67)</favoriteColor>
		<combatPower>22</combatPower>
		<weaponMoney>200</weaponMoney>
		<weaponTags>
			<li>Fae</li>
			<li>Instrument</li>
			<li>NeolithicRangedBasic</li>
			<li>NeolithicMeleeBasic</li>
		</weaponTags>
		<apparelMoney>450</apparelMoney>
		<humanPregnancyChance>0.05</humanPregnancyChance>
		<inventoryOptions>
			<skipChance>0.7</skipChance>
			<subOptionsChooseOne>
				<li>
					<thingDef>Silver</thingDef>
					<countRange>10</countRange>
				</li>
				<li MayRequire="hh.xrushha.AnimaAnimalsCombined">
					<thingDef>AnimaBranch</thingDef>
					<countRange>1~1</countRange>
				</li>
				<li MayRequire="hh.xrushha.AnimaAnimalsCombined">
					<thingDef>AnimaBerry</thingDef>
					<countRange>1~5</countRange>
				</li>
				<li MayRequire="pphhyy.GuldenNew">
					<thingDef>MedicineGuldenHerbal</thingDef>
					<countRange>1~5</countRange>
				</li>
				<li MayRequire="VanillaExpanded.VPlantsEMushrooms">
					<thingDef>VPE_Blisscap</thingDef>
					<countRange>1~5</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
		<modExtensions>
			<li MayRequire="Owlchemist.GiddyUp" Class="GiddyUp.CustomMounts">
				<mountChance>5</mountChance>
				<possibleMounts>
					<li>
						<key>Horse</key>
						<value>25</value>
					</li>
					<li>
						<key>Elephant</key>
						<value>5</value>
					</li>
				</possibleMounts>
			</li>
		</modExtensions>
	</PawnKindDef>
	
