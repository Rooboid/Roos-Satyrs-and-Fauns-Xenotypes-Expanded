<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<KCSG.SettlementLayoutDef>
	<defName>RBSFE_FaeSettlement</defName>
		<!-- Size of the settlement -->
		<settlementSize>62,62</settlementSize>
		<!-- Inner settlement part -->
		<centerBuildings>
			<!-- Inner size, can be the same size -->
			<centerSize>62,62</centerSize>
			<!-- A list of allowed tag -->
			<allowedStructures>
				<li>
					<!-- min~max -->
					<count>-1~3</count>
					<tag>FaeSpecial</tag>
				</li>
				<li>
					<count>2~6</count>
					<tag>FaeGarden</tag>
				</li>
				<li>
					<count>2~6</count>
					<tag>FaeStorage</tag>
				</li>
				<li>
					<count>1~3</count>
					<tag>FaeBedroom</tag>
				</li>
			</allowedStructures>
		</centerBuildings>
		<propsOptions>
			<addMainRoadProps>false</addMainRoadProps>
			<!-- Props spawned on main roads, if any -->
			<mainRoadMinDistance>6</mainRoadMinDistance>
			<!-- Props spawned on link roads, if any -->
			<addLinkRoadProps>false</addLinkRoadProps>
			<linkRoadMinDistance>9</linkRoadMinDistance>
			<!-- Props scattered around -->
			<scatterProps>true</scatterProps>
			<scatterMinDistance>8</scatterMinDistance>
			<scatterMaxAmount>150</scatterMaxAmount>
			<scatterPropsDefs>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_WoodenLogs</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_WoodenLogsLarge</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_WoodenWagon</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_Anvil</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_ArrowCage</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_BucketEmpty</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_BrazierEmpty</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_BucketwithWater</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_BucketwithWater</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_WoodenChest</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_WoodenChestLarge</li>
				<li MayRequire="VanillaExpanded.VFEPropsandDecor">VFEPD_WoodenSign</li>
			</scatterPropsDefs>
		</propsOptions>
		<!-- Defines roads here -->
		<roadOptions>
			<!-- Main road are road that traverse a settlement -->
			<addMainRoad>false</addMainRoad>
			<mainRoadCount>1</mainRoadCount>
			<mainRoadDef>PackedDirt</mainRoadDef>
			<mainRoadWidth>2</mainRoadWidth>
			<!-- Link road are road that link all doors together -->
			<addLinkRoad>true</addLinkRoad>
			<linkRoadDef>PackedDirt</linkRoadDef>
			<linkRoadWidth>2</linkRoadWidth>
		</roadOptions>
		<!-- You can customize the settlement defense here -->
		<defenseOptions>
			<addEdgeDefense>true</addEdgeDefense>
			<addSandbags>false</addSandbags>
			<addTurrets>false</addTurrets>
			<addMortars>false</addMortars>
			<!-- 1 for default number of pawns -->
			<pawnGroupMultiplier>1.4</pawnGroupMultiplier>
		</defenseOptions>
		<stockpileOptions>
			<fillStorageBuildings>true</fillStorageBuildings>
			<fillWithDefs>
				<li>Milk</li>
				<li>MedicineHerbal</li>
				<li>Cloth</li>
				<li>RawPotatoes</li>
				<li>RawRice</li>
				<li>RawBerries</li>
				<li>RawHops</li>
				<li>SmokeleafLeaves</li>
				<li>MealFine</li>
				<li>BabyFood</li>
				<li>WoolAlpaca</li>
				<li>Cloth</li>
				<li>PsychicAnimalPulser</li>
				<li>RBSF_Lyre</li>
				<li>Chocolate</li>
				<li>RBSF_Lute</li>
				<li>RBSFE_Faerrow</li>
				<li>RBSFE_FaeBlade</li>
				<li>RBSFE_FaeStaff</li>
				<li>RBSF_MelodicHealingSongSheet</li>
				<li>RBSF_MelodicSonataSheet</li>
				<li>RBSFE_MelodicHymnSheet</li>
				<li>RBSF_MelodicWarcallSheet</li>
				<li>RBSF_MelodicLullabySheet</li>
				<li MayRequire="Ludeon.Rimworld.Ideology">GauranlenSeed</li>
		
				<li MayRequire="sarg.alphaanimals">AA_AngelMothEggFertilized</li>
				<li MayRequire="sarg.alphaanimals">AA_MothSilk</li>
				<li MayRequire="sarg.alphaanimals">AA_ChameleonYakWoolJungle</li>
				<li MayRequire="sarg.alphaanimals">AA_AgariluxRawFungus</li>
				<li MayRequire="sarg.alphaanimals">AA_BlackTruffles</li>
		
				<li MayRequire="xrushha.AnimaGear">AnimaFiber</li>
		
				<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_BrokenWeapons</li>
				<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_Flour</li>
				<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_Straw</li>
				<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_RawHerb</li>
				<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_RawGrapes</li>
				<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_RawMulberry</li>
				<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_RawLentils</li>
				<li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_ElvenWoodLog</li>
		
				<li MayRequire="VanillaExpanded.VBrewE">VBE_RawTea</li>
				<li MayRequire="VanillaExpanded.VBrewE">VBE_AmbrandyMush</li>
				<li MayRequire="VanillaExpanded.VBrewE">VBE_Whiskey</li>
		
				<li MayRequire="VanillaExpanded.VPlantsE">VCE_RawPepper</li>
				<li MayRequire="VanillaExpanded.VPlantsE">VCE_RawEggplant</li>
				<li MayRequire="VanillaExpanded.VPlantsE">VCE_RawBeets</li>
				<li MayRequire="VanillaExpanded.VPlantsE">VCE_RawBeets</li>
		
			</fillWithDefs>
			<stockpileValueMultiplier>1.7</stockpileValueMultiplier>
		</stockpileOptions>
	</KCSG.SettlementLayoutDef>
  
</Defs>